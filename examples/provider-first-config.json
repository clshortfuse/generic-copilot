{
  "_comment1": "Provider-first Configuration Example",
  "_comment2": "This example demonstrates the provider-first configuration approach with grouped structure",
  "_comment3": "Models automatically inherit baseUrl, headers, and API key from their provider. All other parameters must be explicitly defined on each model.",
  "generic-copilot.providers": [
    {
      "key": "modelscope",
      "displayName": "ModelScope",
      "baseUrl": "https://api-inference.modelscope.cn/v1"
    },
    {
      "key": "siliconflow",
      "displayName": "SiliconFlow",
      "baseUrl": "https://api.siliconflow.cn/v1"
    },
    {
      "key": "zai",
      "displayName": "Zai",
      "baseUrl": "https://open.zaidata.com/v1"
    },
    {
      "key": "openrouter",
      "displayName": "OpenRouter",
      "baseUrl": "https://openrouter.ai/api/v1"
    }
  ],
  "generic-copilot.models": [
    {
      "_comment": "Model with explicit parameters - inherits only baseUrl from provider",
      "model_properties": {
        "id": "Qwen/Qwen3-Coder-480B-A35B-Instruct",
        "provider": "modelscope",
        "context_length": 256000,
        "family": "generic"
      },
      "model_parameters": {
        "max_tokens": 8192,
        "temperature": 0,
        "top_p": 1
      }
    },
    {
      "_comment": "Model with different parameter set",
      "model_properties": {
        "id": "deepseek-ai/DeepSeek-V3",
        "provider": "modelscope",
        "context_length": 256000,
        "family": "generic"
      },
      "model_parameters": {
        "max_tokens": 8192,
        "temperature": 0.5,
        "top_p": 1
      }
    },
    {
      "_comment": "Model with multiple configs for the same base model",
      "model_properties": {
        "id": "glm-4.6",
        "configId": "thinking",
        "provider": "zai",
        "context_length": 256000
      },
      "model_parameters": {
        "max_tokens": 8192,
        "temperature": 0.7,
        "top_p": 1,
        "thinking": {
          "type": "enabled"
        }
      }
    },
    {
      "model_properties": {
        "id": "glm-4.6",
        "configId": "no-thinking",
        "provider": "zai",
        "context_length": 256000
      },
      "model_parameters": {
        "max_tokens": 8192,
        "temperature": 0,
        "top_p": 1,
        "thinking": {
          "type": "disabled"
        }
      }
    },
    {
      "_comment": "Model with reasoning configuration",
      "model_properties": {
        "id": "anthropic/claude-3.5-sonnet",
        "provider": "openrouter",
        "context_length": 200000
      },
      "model_parameters": {
        "max_tokens": 4096,
        "temperature": 0.8,
        "reasoning": {
          "enabled": true,
          "effort": "high"
        }
      }
    },
    {
      "_comment": "Model from siliconflow with vision enabled",
      "model_properties": {
        "id": "Qwen/Qwen2.5-Coder-32B-Instruct",
        "provider": "siliconflow",
        "vision": true,
        "context_length": 128000
      },
      "model_parameters": {
        "max_tokens": 16384,
        "temperature": 0.7,
        "top_p": 0.95
      }
    },
    {
      "_comment": "Model config without provider reference",
      "model_properties": {
        "id": "custom-model",
        "owned_by": "custom-provider",
        "baseUrl": "https://api.custom-provider.com/v1",
        "context_length": 128000
      },
      "model_parameters": {
        "max_tokens": 4096,
        "temperature": 0.8
      }
    }
  ]
}